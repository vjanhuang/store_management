{"ast":null,"code":"export default {\n  name: \"Storein\",\n  data() {\n    return {\n      tableData: [],\n      goods: [],\n      stashs: [],\n      total: 0,\n      pageNum: 1,\n      name: \"\",\n      form: {},\n      dialogFormVisible: false,\n      multipleSelection: [],\n      user: localStorage.getItem(\"user\") ? JSON.parse(localStorage.getItem(\"user\")) : {},\n      rules: {\n        name: [{\n          required: true,\n          message: '请输入必填项',\n          trigger: 'blur'\n        }]\n      }\n    };\n  },\n  created() {\n    this.load();\n    this.request.get(\"/stash\").then(res => {\n      this.stashs = res.data;\n    });\n    this.request.get(\"/goods\").then(res => {\n      this.goods = res.data;\n    });\n  },\n  methods: {\n    load() {\n      this.request.get(\"/storein/page\", {\n        params: {\n          pageNum: this.pageNum,\n          pageSize: 10,\n          name: this.name\n        }\n      }).then(res => {\n        this.tableData = res.data?.records;\n        this.total = res.data?.total;\n      });\n    },\n    save() {\n      this.$refs['ruleForm'].validate(valid => {\n        if (valid) {\n          this.form.code = this.stashs.find(v => v.name === this.form.name).code;\n          this.form.goodscode = this.goods.find(v => v.name === this.form.goods).code;\n          this.request({\n            method: this.form.id ? 'PUT' : 'POST',\n            url: \"/storein\",\n            data: this.form\n          }).then(res => {\n            if (res.code === '200') {\n              this.$message.success(\"操作成功\");\n              this.dialogFormVisible = false;\n              this.load();\n            } else {\n              this.$message.error(res.msg);\n            }\n          });\n        }\n      });\n    },\n    handleAdd() {\n      this.dialogFormVisible = true;\n      this.form = {};\n      this.$nextTick(() => {\n        if (this.$refs.img) {\n          this.$refs.img.clearFiles();\n        }\n        if (this.$refs.file) {\n          this.$refs.file.clearFiles();\n        }\n      });\n    },\n    handleEdit(row) {\n      this.form = JSON.parse(JSON.stringify(row));\n      this.dialogFormVisible = true;\n      this.$nextTick(() => {\n        if (this.$refs.img) {\n          this.$refs.img.clearFiles();\n        }\n        if (this.$refs.file) {\n          this.$refs.file.clearFiles();\n        }\n      });\n    },\n    del(id) {\n      this.request.delete(\"/storein/\" + id).then(res => {\n        if (res.code === '200') {\n          this.$message.success(\"操作成功\");\n          this.load();\n        } else {\n          this.$message.error(res.msg);\n        }\n      });\n    },\n    handleSelectionChange(val) {\n      console.log(val);\n      this.multipleSelection = val;\n    },\n    delBatch() {\n      if (!this.multipleSelection.length) {\n        this.$message.error(\"请选择需要删除的数据\");\n        return;\n      }\n      let ids = this.multipleSelection.map(v => v.id); // [{}, {}, {}] => [1,2,3]\n      this.request.post(\"/storein/del/batch\", ids).then(res => {\n        if (res.code === '200') {\n          this.$message.success(\"操作成功\");\n          this.load();\n        } else {\n          this.$message.error(res.msg);\n        }\n      });\n    },\n    reset() {\n      this.name = \"\";\n      this.load();\n    },\n    handleCurrentChange(pageNum) {\n      this.pageNum = pageNum;\n      this.load();\n    },\n    handleFileUploadSuccess(res) {\n      this.form.file = res;\n    },\n    handleImgUploadSuccess(res) {\n      this.form.img = res;\n    },\n    download(url) {\n      window.open(url);\n    },\n    exp() {\n      window.open($baseUrl + \"/storein/export\");\n    },\n    handleExcelImportSuccess() {\n      this.$message.success(\"导入成功\");\n      this.load();\n    }\n  }\n};","map":{"version":3,"names":["name","data","tableData","goods","stashs","total","pageNum","form","dialogFormVisible","multipleSelection","user","localStorage","getItem","JSON","parse","rules","required","message","trigger","created","load","request","get","then","res","methods","params","pageSize","records","save","$refs","validate","valid","code","find","v","goodscode","method","id","url","$message","success","error","msg","handleAdd","$nextTick","img","clearFiles","file","handleEdit","row","stringify","del","delete","handleSelectionChange","val","console","log","delBatch","length","ids","map","post","reset","handleCurrentChange","handleFileUploadSuccess","handleImgUploadSuccess","download","window","open","exp","$baseUrl","handleExcelImportSuccess"],"sources":["src/views/manager/Storein.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <div style=\"margin-bottom: 10px\">\r\n      <el-input style=\"width: 260px\" placeholder=\"请输入...\" v-model=\"name\"></el-input>\r\n      <el-button style=\"margin-left: 10px\" type=\"primary\" @click=\"load\">搜 索</el-button>\r\n      <el-button type=\"info\" @click=\"reset\">重 置</el-button>\r\n    </div>\r\n\r\n    <div style=\"margin: 10px 0\">\r\n      <el-button type=\"primary\" @click=\"handleAdd\">新 增</el-button>\r\n      <el-popconfirm\r\n        style=\"margin-left: 10px\"\r\n        confirm-button-text='确 定'\r\n        cancel-button-text='我再想想'\r\n        icon=\"el-icon-info\"\r\n        icon-color=\"red\"\r\n        title=\"您确定批量删除这些数据吗？\"\r\n        @confirm=\"delBatch\"\r\n      >\r\n        <el-button type=\"danger\" slot=\"reference\">批量删除</el-button>\r\n      </el-popconfirm>\r\n    </div>\r\n\r\n    <el-table :data=\"tableData\" stripe :header-cell-style=\"{background:'#f4f3f9',color:'#606266'}\"  @selection-change=\"handleSelectionChange\">\r\n      <el-table-column type=\"selection\" width=\"55\" align=\"center\"></el-table-column>\r\n      <el-table-column prop=\"id\" label=\"序号\" width=\"80\" align=\"center\" sortable></el-table-column>\r\n      <el-table-column align=\"center\" prop=\"name\" label=\"仓库名称\"></el-table-column>\r\n      <el-table-column align=\"center\" prop=\"code\" label=\"仓库编号\"></el-table-column>\r\n      <el-table-column align=\"center\" prop=\"goods\" label=\"物品名称\"></el-table-column>\r\n      <el-table-column align=\"center\" prop=\"goodscode\" label=\"物品编号\"></el-table-column>\r\n      <el-table-column align=\"center\" prop=\"num\" label=\"库存量\"></el-table-column>\r\n      <el-table-column align=\"center\" prop=\"date\" label=\"日期\"></el-table-column>\r\n\r\n      <el-table-column label=\"操作\" width=\"180\" align=\"center\">\r\n        <template slot-scope=\"scope\">\r\n<!--          <el-button type=\"primary\" @click=\"handleEdit(scope.row)\" size=\"mini\">编辑</el-button>-->\r\n          <el-popconfirm\r\n              style=\"margin-left: 10px\"\r\n              confirm-button-text='确定'\r\n              cancel-button-text='我再想想'\r\n              icon=\"el-icon-info\"\r\n              icon-color=\"red\"\r\n              title=\"您确定删除吗？\"\r\n              @confirm=\"del(scope.row.id)\"\r\n          >\r\n            <el-button type=\"danger\" slot=\"reference\" size=\"mini\">删除</el-button>\r\n          </el-popconfirm>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n\r\n    <div style=\"margin: 10px 0\">\r\n      <el-pagination\r\n          style=\"padding: 0\"\r\n          background\r\n          layout=\"total, prev, pager, next\"\r\n          @current-change=\"handleCurrentChange\"\r\n          :current-page=\"pageNum\"\r\n          :total=\"total\">\r\n      </el-pagination>\r\n    </div>\r\n\r\n    <el-dialog title=\"信息\" :visible.sync=\"dialogFormVisible\" width=\"30%\" :close-on-click-modal=\"false\">\r\n      <el-form label-width=\"100px\" style=\"padding-right: 40px\" :model=\"form\" :rules=\"rules\" ref=\"ruleForm\">\r\n        <el-form-item label=\"仓库\">\r\n          <el-select clearable v-model=\"form.name\" placeholder=\"请选择\"  style=\"width: 100%\">\r\n            <el-option v-for=\"item in stashs\" :key=\"item.id\" :label=\"item.name\" :value=\"item.name\"></el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n        <el-form-item label=\"物品\">\r\n          <el-select clearable v-model=\"form.goods\" placeholder=\"请选择\"  style=\"width: 100%\">\r\n            <el-option v-for=\"item in goods\" :key=\"item.id\" :label=\"item.name\" :value=\"item.name\"></el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n        <el-form-item prop=\"num\" label=\"新增库存\">\r\n          <el-input v-model=\"form.num\" autocomplete=\"off\"></el-input>\r\n        </el-form-item>\r\n\r\n      </el-form>\r\n      <div slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"dialogFormVisible = false\">取 消</el-button>\r\n        <el-button type=\"primary\" @click=\"save\">确 定</el-button>\r\n      </div>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"Storein\",\r\n  data() {\r\n    return {\r\n      tableData: [],\r\n      goods: [],\r\n      stashs: [],\r\n      total: 0,\r\n      pageNum: 1,\r\n      name: \"\",\r\n      form: {},\r\n      dialogFormVisible: false,\r\n      multipleSelection: [],\r\n      user: localStorage.getItem(\"user\") ? JSON.parse(localStorage.getItem(\"user\")) : {},\r\n      rules: {\r\n        name: [\r\n          { required: true, message: '请输入必填项', trigger: 'blur'}\r\n        ],\r\n      }\r\n    }\r\n  },\r\n  created() {\r\n    this.load()\r\n    this.request.get(\"/stash\").then(res => {\r\n      this.stashs = res.data\r\n    })\r\n    this.request.get(\"/goods\").then(res => {\r\n      this.goods = res.data\r\n    })\r\n  },\r\n  methods: {\r\n    load() {\r\n      this.request.get(\"/storein/page\", {\r\n        params: {\r\n          pageNum: this.pageNum,\r\n          pageSize: 10,\r\n          name: this.name,\r\n        }\r\n      }).then(res => {\r\n        this.tableData = res.data?.records\r\n        this.total = res.data?.total\r\n      })\r\n    },\r\n    save() {\r\n        this.$refs['ruleForm'].validate((valid) => {\r\n          if (valid) {\r\n            this.form.code = this.stashs.find(v => v.name === this.form.name).code\r\n            this.form.goodscode = this.goods.find(v => v.name === this.form.goods).code\r\n           this.request({\r\n             method: this.form.id ? 'PUT' : 'POST',\r\n             url: \"/storein\",\r\n             data: this.form\r\n           }).then(res => {\r\n              if (res.code === '200') {\r\n                this.$message.success(\"操作成功\")\r\n                this.dialogFormVisible = false\r\n                this.load()\r\n              } else {\r\n                this.$message.error(res.msg)\r\n              }\r\n            })\r\n          }\r\n        })\r\n    },\r\n    handleAdd() {\r\n      this.dialogFormVisible = true\r\n      this.form = {}\r\n      this.$nextTick(() => {\r\n        if(this.$refs.img) {\r\n           this.$refs.img.clearFiles();\r\n         }\r\n         if(this.$refs.file) {\r\n          this.$refs.file.clearFiles();\r\n         }\r\n      })\r\n    },\r\n    handleEdit(row) {\r\n      this.form = JSON.parse(JSON.stringify(row))\r\n      this.dialogFormVisible = true\r\n       this.$nextTick(() => {\r\n         if(this.$refs.img) {\r\n           this.$refs.img.clearFiles();\r\n         }\r\n         if(this.$refs.file) {\r\n          this.$refs.file.clearFiles();\r\n         }\r\n       })\r\n    },\r\n    del(id) {\r\n      this.request.delete(\"/storein/\" + id).then(res => {\r\n        if (res.code === '200') {\r\n          this.$message.success(\"操作成功\")\r\n          this.load()\r\n        } else {\r\n          this.$message.error(res.msg)\r\n        }\r\n      })\r\n    },\r\n    handleSelectionChange(val) {\r\n      console.log(val)\r\n      this.multipleSelection = val\r\n    },\r\n    delBatch() {\r\n      if (!this.multipleSelection.length) {\r\n        this.$message.error(\"请选择需要删除的数据\")\r\n        return\r\n      }\r\n      let ids = this.multipleSelection.map(v => v.id)  // [{}, {}, {}] => [1,2,3]\r\n      this.request.post(\"/storein/del/batch\", ids).then(res => {\r\n        if (res.code === '200') {\r\n          this.$message.success(\"操作成功\")\r\n          this.load()\r\n        } else {\r\n          this.$message.error(res.msg)\r\n        }\r\n      })\r\n    },\r\n    reset() {\r\n      this.name = \"\"\r\n      this.load()\r\n    },\r\n    handleCurrentChange(pageNum) {\r\n      this.pageNum = pageNum\r\n      this.load()\r\n    },\r\n    handleFileUploadSuccess(res) {\r\n      this.form.file = res\r\n    },\r\n    handleImgUploadSuccess(res) {\r\n      this.form.img = res\r\n    },\r\n    download(url) {\r\n      window.open(url)\r\n    },\r\n    exp() {\r\n      window.open($baseUrl + \"/storein/export\")\r\n    },\r\n    handleExcelImportSuccess() {\r\n      this.$message.success(\"导入成功\")\r\n      this.load()\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n\r\n<style scope>\r\n\r\n</style>\r\n"],"mappings":"AAwFA;EACAA,IAAA;EACAC,KAAA;IACA;MACAC,SAAA;MACAC,KAAA;MACAC,MAAA;MACAC,KAAA;MACAC,OAAA;MACAN,IAAA;MACAO,IAAA;MACAC,iBAAA;MACAC,iBAAA;MACAC,IAAA,EAAAC,YAAA,CAAAC,OAAA,WAAAC,IAAA,CAAAC,KAAA,CAAAH,YAAA,CAAAC,OAAA;MACAG,KAAA;QACAf,IAAA,GACA;UAAAgB,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA;MAEA;IACA;EACA;EACAC,QAAA;IACA,KAAAC,IAAA;IACA,KAAAC,OAAA,CAAAC,GAAA,WAAAC,IAAA,CAAAC,GAAA;MACA,KAAApB,MAAA,GAAAoB,GAAA,CAAAvB,IAAA;IACA;IACA,KAAAoB,OAAA,CAAAC,GAAA,WAAAC,IAAA,CAAAC,GAAA;MACA,KAAArB,KAAA,GAAAqB,GAAA,CAAAvB,IAAA;IACA;EACA;EACAwB,OAAA;IACAL,KAAA;MACA,KAAAC,OAAA,CAAAC,GAAA;QACAI,MAAA;UACApB,OAAA,OAAAA,OAAA;UACAqB,QAAA;UACA3B,IAAA,OAAAA;QACA;MACA,GAAAuB,IAAA,CAAAC,GAAA;QACA,KAAAtB,SAAA,GAAAsB,GAAA,CAAAvB,IAAA,EAAA2B,OAAA;QACA,KAAAvB,KAAA,GAAAmB,GAAA,CAAAvB,IAAA,EAAAI,KAAA;MACA;IACA;IACAwB,KAAA;MACA,KAAAC,KAAA,aAAAC,QAAA,CAAAC,KAAA;QACA,IAAAA,KAAA;UACA,KAAAzB,IAAA,CAAA0B,IAAA,QAAA7B,MAAA,CAAA8B,IAAA,CAAAC,CAAA,IAAAA,CAAA,CAAAnC,IAAA,UAAAO,IAAA,CAAAP,IAAA,EAAAiC,IAAA;UACA,KAAA1B,IAAA,CAAA6B,SAAA,QAAAjC,KAAA,CAAA+B,IAAA,CAAAC,CAAA,IAAAA,CAAA,CAAAnC,IAAA,UAAAO,IAAA,CAAAJ,KAAA,EAAA8B,IAAA;UACA,KAAAZ,OAAA;YACAgB,MAAA,OAA