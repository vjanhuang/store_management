eries: [\r\n    {\r\n      name: '入库',\r\n      type: 'line',\r\n      smooth: true,\r\n      data: []\r\n    },\r\n    {\r\n      name: '出库',\r\n      type: 'line',\r\n      smooth: true,\r\n      data: []\r\n    },\r\n    {\r\n      name: '总库存',\r\n      type: 'line',\r\n      smooth: true,\r\n      data: []\r\n    },\r\n  ]\r\n};\r\n\r\nexport default {\r\n  name: 'Home',\r\n  data() {\r\n    return {\r\n      data: {},\r\n      user: localStorage.getItem(\"user\") ? JSON.parse(localStorage.getItem(\"user\")) : {},\r\n    }\r\n  },\r\n  mounted() {\r\n    this.request.get('/data').then(res => this.data = res?.data)\r\n\r\n    const chartDom = document.getElementById('main')\r\n    const myChart = echarts.init(chartDom)\r\n    this.request.get('/line').then(res => {\r\n      option.series[0].data = res.data?.in\r\n      option.series[1].data = res.data?.out\r\n      option.series[2].data = res.data?.store\r\n      option.xAxis.data = res.data?.dates\r\n      myChart.setOption(option)\r\n    })\r\n\r\n\r\n  },\r\n  methods: {}\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.box {\r\n  width: 100%;\r\n  box-shadow: 0 0 10px -2px rgba(0, 0, 0, .2);\r\n  border-radius: 5px;\r\n  padding: 20px;\r\n  text-align: center;\r\n}\r\n</style>\r\n"],"mappings":"AAoDA,YAAAA,OAAA;AAEA,MAAAC,MAAA;EACAC,KAAA;IACAC,IAAA;IACAC,IAAA;EACA;EACAC,OAAA;IACAC,OAAA;EACA;EACAC,MAAA;IACAC,IAAA;EACA;EACAC,IAAA;IACAL,IAAA;IACAM,KAAA;IACAC,MAAA;IACAC,YAAA;EACA;EACA;EACA;EACA;EACA;EACA;EACAC,KAAA;IACAC,IAAA;IACAC,WAAA;IACAP,IAAA;EACA;EACAQ,KAAA;IACAF,IAAA;EACA;EACAG,MAAA,GACA;IACAC,IAAA;IACAJ,IAAA;IACAK,MAAA;IACAX,IAAA;EACA,GACA;IACAU,IAAA;IACAJ,IAAA;IACAK,MAAA;IACAX,IAAA;EACA,GACA;IACAU,IAAA;IACAJ,IAAA;IACAK,MAAA;IACAX,IAAA;EACA;AAEA;AAEA;EACAU,IAAA;EACAV,KAAA;IACA;MACAA,IAAA;MACAY,IAAA,EAAAC,YAAA,CAAAC,OAAA,WAAAC,IAAA,CAAAC,KAAA,CAAAH,YAAA,CAAAC,OAAA;IACA;EACA;EACAG,QAAA;IACA,KAAAC,OAAA,CAAAC,GAAA,UAAAC,IAAA,CAAAC,GAAA,SAAArB,IAAA,GAAAqB,GAAA,EAAArB,IAAA;IAEA,MAAAsB,QAAA,GAAAC,QAAA,CAAAC,cAAA;IACA