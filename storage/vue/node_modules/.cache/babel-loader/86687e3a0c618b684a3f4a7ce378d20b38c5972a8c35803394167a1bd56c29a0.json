{"ast":null,"code":"export default {\n  name: \"User\",\n  data() {\n    return {\n      tableData: [],\n      total: 0,\n      pageNum: 1,\n      name: \"\",\n      form: {},\n      dialogFormVisible: false,\n      multipleSelection: [],\n      user: localStorage.getItem(\"user\") ? JSON.parse(localStorage.getItem(\"user\")) : {},\n      rules: {\n        username: [{\n          required: true,\n          message: '请输入必填项',\n          trigger: 'blur'\n        }],\n        name: [{\n          required: true,\n          message: '请输入必填项',\n          trigger: 'blur'\n        }]\n      }\n    };\n  },\n  created() {\n    this.load();\n  },\n  methods: {\n    load() {\n      this.request.get(\"/user/page\", {\n        params: {\n          pageNum: this.pageNum,\n          pageSize: 10,\n          name: this.name\n        }\n      }).then(res => {\n        this.tableData = res.data?.records;\n        this.total = res.data.total;\n      });\n    },\n    save() {\n      this.$refs['ruleForm'].validate(valid => {\n        if (valid) {\n          this.request({\n            method: this.form.id ? 'PUT' : 'POST',\n            url: \"/user\",\n            data: this.form\n          }).then(res => {\n            if (res.code === '200') {\n              this.$message.success(\"操作成功\");\n              this.dialogFormVisible = false;\n              this.load();\n            } else {\n              this.$message.error(res.msg);\n            }\n          });\n        }\n      });\n    },\n    handleAdd() {\n      this.dialogFormVisible = true;\n      this.form = {};\n      this.$nextTick(() => {\n        if (this.$refs.img) {\n          this.$refs.img.clearFiles();\n        }\n        if (this.$refs.file) {\n          this.$refs.file.clearFiles();\n        }\n      });\n    },\n    handleEdit(row) {\n      this.form = JSON.parse(JSON.stringify(row));\n      this.dialogFormVisible = true;\n      this.$nextTick(() => {\n        if (this.$refs.img) {\n          this.$refs.img.clearFiles();\n        }\n        if (this.$refs.file) {\n          this.$refs.file.clearFiles();\n        }\n      });\n    },\n    del(id) {\n      this.request.delete(\"/user/\" + id).then(res => {\n        if (res.code === '200') {\n          this.$message.success(\"操作成功\");\n          this.load();\n        } else {\n          this.$message.error(res.msg);\n        }\n      });\n    },\n    handleSelectionChange(val) {\n      console.log(val);\n      this.multipleSelection = val;\n    },\n    delBatch() {\n      if (!this.multipleSelection.length) {\n        this.$message.error(\"请选择需要删除的数据\");\n        return;\n      }\n      let ids = this.multipleSelection.map(v => v.id); // [{}, {}, {}] => [1,2,3]\n      this.request.post(\"/user/del/batch\", ids).then(res => {\n        if (res.code === '200') {\n          this.$message.success(\"操作成功\");\n          this.load();\n        } else {\n          this.$message.error(res.msg);\n        }\n      });\n    },\n    reset() {\n      this.name = \"\";\n      this.load();\n    },\n    handleCurrentChange(pageNum) {\n      this.pageNum = pageNum;\n      this.load();\n    },\n    resetPass(row) {\n      row.password = '123';\n      this.updateUser(row);\n    },\n    disableUser(row) {\n      let obj = Object.assign({}, row);\n      obj.disable = !!obj.disable;\n      this.updateUser(obj);\n    },\n    updateUser(user) {\n      this.request({\n        method: 'PUT',\n        url: \"/user\",\n        data: user\n      }).then(res => {\n        if (res.code === '200') {\n          this.$message.success(\"操作成功\");\n          this.load();\n        } else {\n          this.$message.error(res.msg);\n        }\n      });\n    },\n    handleFileUploadSuccess(res) {\n      this.form.file = res;\n    },\n    handleImgUploadSuccess(res) {\n      this.form.img = res;\n    },\n    download(url) {\n      window.open(url);\n    },\n    exp() {\n      window.open($baseUrl + \"/user/export\");\n    },\n    handleExcelImportSuccess() {\n      this.$message.success(\"导入成功\");\n      this.load();\n    }\n  }\n};","map":{"version":3,"names":["name","data","tableData","total","pageNum","form","dialogFormVisible","multipleSelection","user","localStorage","getItem","JSON","parse","rules","username","required","message","trigger","created","load","methods","request","get","params","pageSize","then","res","records","save","$refs","validate","valid","method","id","url","code","$message","success","error","msg","handleAdd","$nextTick","img","clearFiles","file","handleEdit","row","stringify","del","delete","handleSelectionChange","val","console","log","delBatch","length","ids","map","v","post","reset","handleCurrentChange","resetPass","password","updateUser","disableUser","obj","Object","assign","disable","handleFileUploadSuccess","handleImgUploadSuccess","download","window","open","exp","$baseUrl","handleExcelImportSuccess"],"sources":["src/views/manager/User.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <div style=\"margin-bottom: 10px\">\r\n      <el-input style=\"width: 260px\" placeholder=\"请输入...\" v-model=\"name\"></el-input>\r\n      <el-button style=\"margin-left: 10px\" type=\"primary\" @click=\"load\">搜 索</el-button>\r\n      <el-button type=\"info\" @click=\"reset\">重 置</el-button>\r\n    </div>\r\n\r\n    <div style=\"margin: 10px 0\">\r\n      <el-button type=\"primary\" @click=\"handleAdd\">新 增</el-button>\r\n      <el-popconfirm\r\n        style=\"margin-left: 10px\"\r\n        confirm-button-text='确 定'\r\n        cancel-button-text='我再想想'\r\n        icon=\"el-icon-info\"\r\n        icon-color=\"red\"\r\n        title=\"您确定批量删除这些数据吗？\"\r\n        @confirm=\"delBatch\"\r\n      >\r\n        <el-button type=\"danger\" slot=\"reference\">批量删除</el-button>\r\n      </el-popconfirm>\r\n    </div>\r\n\r\n    <el-table :data=\"tableData\" stripe :header-cell-style=\"{background:'#f4f3f9',color:'#606266'}\"  @selection-change=\"handleSelectionChange\">\r\n      <el-table-column type=\"selection\" width=\"55\" align=\"center\"></el-table-column>\r\n      <el-table-column prop=\"id\" label=\"序号\" width=\"80\" align=\"center\" sortable></el-table-column>\r\n      <el-table-column align=\"center\" prop=\"username\" label=\"用户名\"></el-table-column>\r\n<!--      <el-table-column align=\"center\" prop=\"password\" label=\"密码\"></el-table-column>-->\r\n      <el-table-column align=\"center\" prop=\"name\" label=\"名称\"></el-table-column>\r\n      <el-table-column align=\"center\" prop=\"sex\" label=\"性别\"></el-table-column>\r\n      <el-table-column align=\"center\" prop=\"birth\" label=\"生日\"></el-table-column>\r\n      <el-table-column align=\"center\" prop=\"phone\" label=\"手机\"></el-table-column>\r\n      <el-table-column align=\"center\" prop=\"address\" label=\"地址\"></el-table-column>\r\n      <el-table-column label=\"禁用\">\r\n        <template v-slot=\"scope\">\r\n          <el-switch v-model=\"scope.row.disable\" active-color=\"red\" @change=\"disableUser(scope.row)\"></el-switch>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column label=\"操作\" width=\"240\" align=\"center\">\r\n        <template slot-scope=\"scope\">\r\n          <el-button type=\"primary\" @click=\"handleEdit(scope.row)\" size=\"mini\">编辑</el-button>\r\n          <el-button type=\"warning\" @click=\"resetPass(scope.row)\" size=\"mini\">重置密码</el-button>\r\n          <el-popconfirm\r\n              style=\"margin-left: 10px\"\r\n              confirm-button-text='确定'\r\n              cancel-button-text='我再想想'\r\n              icon=\"el-icon-info\"\r\n              icon-color=\"red\"\r\n              title=\"您确定删除吗？\"\r\n              @confirm=\"del(scope.row.id)\"\r\n          >\r\n            <el-button type=\"danger\" slot=\"reference\" size=\"mini\">删除</el-button>\r\n          </el-popconfirm>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n\r\n    <div style=\"margin: 10px 0\">\r\n      <el-pagination\r\n          style=\"padding: 0\"\r\n          background\r\n          layout=\"total, prev, pager, next\"\r\n          @current-change=\"handleCurrentChange\"\r\n          :current-page=\"pageNum\"\r\n          :total=\"total\">\r\n      </el-pagination>\r\n    </div>\r\n\r\n    <el-dialog title=\"信息\" :visible.sync=\"dialogFormVisible\" width=\"30%\" :close-on-click-modal=\"false\">\r\n      <el-form label-width=\"100px\" style=\"padding-right: 40px\" :model=\"form\" :rules=\"rules\" ref=\"ruleForm\">\r\n        <el-form-item prop=\"username\" label=\"用户名\">\r\n          <el-input v-model=\"form.username\" autocomplete=\"off\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item prop=\"password\" label=\"密码\">\r\n          <el-input v-model=\"form.password\" autocomplete=\"off\" disabled></el-input>\r\n        </el-form-item>\r\n        <el-form-item prop=\"name\" label=\"名称\">\r\n          <el-input v-model=\"form.name\" autocomplete=\"off\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item prop=\"sex\" label=\"性别\">\r\n          <el-input v-model=\"form.sex\" autocomplete=\"off\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item prop=\"birth\" label=\"生日\">\r\n          <el-input v-model=\"form.birth\" autocomplete=\"off\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item prop=\"phone\" label=\"手机\">\r\n          <el-input v-model=\"form.phone\" autocomplete=\"off\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item prop=\"address\" label=\"地址\">\r\n          <el-input type=\"textarea\" v-model=\"form.address\" autocomplete=\"off\"></el-input>\r\n        </el-form-item>\r\n\r\n      </el-form>\r\n      <div slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"dialogFormVisible = false\">取 消</el-button>\r\n        <el-button type=\"primary\" @click=\"save\">确 定</el-button>\r\n      </div>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"User\",\r\n  data() {\r\n    return {\r\n      tableData: [],\r\n      total: 0,\r\n      pageNum: 1,\r\n      name: \"\",\r\n      form: {},\r\n      dialogFormVisible: false,\r\n      multipleSelection: [],\r\n      user: localStorage.getItem(\"user\") ? JSON.parse(localStorage.getItem(\"user\")) : {},\r\n      rules: {\r\n        username: [\r\n          { required: true, message: '请输入必填项', trigger: 'blur'}\r\n        ],\r\n        name: [\r\n          { required: true, message: '请输入必填项', trigger: 'blur'}\r\n        ],\r\n      }\r\n    }\r\n  },\r\n  created() {\r\n    this.load()\r\n  },\r\n  methods: {\r\n    load() {\r\n      this.request.get(\"/user/page\", {\r\n        params: {\r\n          pageNum: this.pageNum,\r\n          pageSize: 10,\r\n          name: this.name,\r\n        }\r\n      }).then(res => {\r\n        this.tableData = res.data?.records\r\n        this.total = res.data.total\r\n      })\r\n    },\r\n    save() {\r\n        this.$refs['ruleForm'].validate((valid) => {\r\n          if (valid) {\r\n           this.request({\r\n             method: this.form.id ? 'PUT' : 'POST',\r\n             url: \"/user\",\r\n             data: this.form\r\n           }).then(res => {\r\n              if (res.code === '200') {\r\n                this.$message.success(\"操作成功\")\r\n                this.dialogFormVisible = false\r\n                this.load()\r\n              } else {\r\n                this.$message.error(res.msg)\r\n              }\r\n            })\r\n          }\r\n        })\r\n    },\r\n    handleAdd() {\r\n      this.dialogFormVisible = true\r\n      this.form = {}\r\n      this.$nextTick(() => {\r\n        if(this.$refs.img) {\r\n           this.$refs.img.clearFiles();\r\n         }\r\n         if(this.$refs.file) {\r\n          this.$refs.file.clearFiles();\r\n         }\r\n      })\r\n    },\r\n    handleEdit(row) {\r\n      this.form = JSON.parse(JSON.stringify(row))\r\n      this.dialogFormVisible = true\r\n       this.$nextTick(() => {\r\n         if(this.$refs.img) {\r\n           this.$refs.img.clearFiles();\r\n         }\r\n         if(this.$refs.file) {\r\n          this.$refs.file.clearFiles();\r\n         }\r\n       })\r\n    },\r\n    del(id) {\r\n      this.request.delete(\"/user/\" + id).then(res => {\r\n        if (res.code === '200') {\r\n          this.$message.success(\"操作成功\")\r\n          this.load()\r\n        } else {\r\n          this.$message.error(res.msg)\r\n        }\r\n      })\r\n    },\r\n    handleSelectionChange(val) {\r\n      console.log(val)\r\n      this.multipleSelection = val\r\n    },\r\n    delBatch() {\r\n      if (!this.multipleSelection.length) {\r\n        this.$message.error(\"请选择需要删除的数据\")\r\n        return\r\n      }\r\n      let ids = this.multipleSelection.map(v => v.id)  // [{}, {}, {}] => [1,2,3]\r\n      this.request.post(\"/user/del/batch\", ids).then(res => {\r\n        if (res.code === '200') {\r\n          this.$message.success(\"操作成功\")\r\n          this.load()\r\n        } else {\r\n          this.$message.error(res.msg)\r\n        }\r\n      })\r\n    },\r\n    reset() {\r\n      this.name = \"\"\r\n      this.load()\r\n    },\r\n    handleCurrentChange(pageNum) {\r\n      this.pageNum = pageNum\r\n      this.load()\r\n    },\r\n    resetPass(row) {\r\n      row.password = '123'\r\n      this.updateUser(row)\r\n    },\r\n    disableUser(row) {\r\n      let obj = Object.assign({}, row)\r\n      obj.disable = !!obj.disable\r\n      this.updateUser(obj)\r\n    },\r\n    updateUser(user) {\r\n      this.request({\r\n        method: 'PUT',\r\n        url: \"/user\",\r\n        data: user\r\n      }).then(res => {\r\n        if (res.code === '200') {\r\n          this.$message.success(\"操作成功\")\r\n          this.load()\r\n        } else {\r\n          this.$message.error(res.msg)\r\n        }\r\n      })\r\n    },\r\n    handleFileUploadSuccess(res) {\r\n      this.form.file = res\r\n    },\r\n    handleImgUploadSuccess(res) {\r\n      this.form.img = res\r\n    },\r\n    download(url) {\r\n      window.open(url)\r\n    },\r\n    exp() {\r\n      window.open($baseUrl + \"/user/export\")\r\n    },\r\n    handleExcelImportSuccess() {\r\n      this.$message.success(\"导入成功\")\r\n      this.load()\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n\r\n<style scope>\r\n\r\n</style>\r\n"],"mappings":"AAsGA;EACAA,IAAA;EACAC,KAAA;IACA;MACAC,SAAA;MACAC,KAAA;MACAC,OAAA;MACAJ,IAAA;MACAK,IAAA;MACAC,iBAAA;MACAC,iBAAA;MACAC,IAAA,EAAAC,YAAA,CAAAC,OAAA,WAAAC,IAAA,CAAAC,KAAA,CAAAH,YAAA,CAAAC,OAAA;MACAG,KAAA;QACAC,QAAA,GACA;UAAAC,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA,EACA;QACAjB,IAAA,GACA;UAAAe,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA;MAEA;IACA;EACA;EACAC,QAAA;IACA,KAAAC,IAAA;EACA;EACAC,OAAA;IACAD,KAAA;MACA,KAAAE,OAAA,CAAAC,GAAA;QACAC,MAAA;UACAnB,OAAA,OAAAA,OAAA;UACAoB,QAAA;UACAxB,IAAA,OAAAA;QACA;MACA,GAAAyB,IAAA,CAAAC,GAAA;QACA,KAAAxB,SAAA,GAAAwB,GAAA,CAAAzB,IAAA,EAAA0B,OAAA;QACA,KAAAxB,KAAA,GAAAuB,GAAA,CAAAzB,IAAA,CAAAE,KAAA;MACA;IACA;IACAyB,KAAA;MACA,KAAAC,KAAA,aAAAC,QAAA,CAAAC,KAAA;QACA,IAAAA,KAAA;UACA,KAAAV,OAAA;YACAW,MAAA,OAAA3B,IAAA,CAAA4B,EAAA;YACAC,GAAA;YACAjC,IAAA,OAAAI;UACA,GAAAoB,IAAA,CAAAC,GAAA;YACA,IAAAA,GAAA,CAAAS,IAAA;cACA,KAAAC,QAAA,CAAAC,OAAA;cACA,KAAA/B,iBAAA;cACA,KAAAa,IAAA;YACA;cACA,KAAAiB,QAAA,CAAAE,KAAA,CAAAZ,GAAA,CAAAa,GAAA;YACA;UACA;QACA;MACA;IACA;IACAC,UAAA;MACA,KAAAlC,iBAAA;MACA,KAAAD,IAAA;MACA,KAAAoC,SAAA;QACA,SAAAZ,KAAA,CAAAa,GAAA;UACA,KAAAb,KAAA,CAAAa,GAAA,CAAAC,UAAA;QACA;QACA,SAAAd,KAAA,CAAAe,IAAA;UACA,KAAAf,KAAA,CAAAe,IAAA,CAAAD,UAAA;QACA;MACA;IACA;IACAE,WAAAC,GAAA;MACA,KAAAzC,IAAA,GAAAM,IAAA,CAAAC,KAAA,CAAAD,IAAA,CAAAoC,SAAA,CAAAD,GAAA;MACA,KAAAxC,iBAAA;MACA,KAAAmC,SAAA;QACA,SAAAZ,KAAA,CAAAa,GAAA;UACA,KAAAb,KAAA,CAAAa,GAAA,CAAAC,UAAA;QACA;QACA,SAAAd,KAAA,CAAAe,IAAA;UACA,KAAAf,KAAA,CAAAe,IAAA,CAAAD,UAAA;QACA;MACA;IACA;IACAK,IAAAf,EAAA;MACA,KAAAZ,OAAA,CAAA4B,MAAA,YAAAhB,EAAA,EAAAR,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAS,IAAA;UACA,KAAAC,QAAA,CAAAC,OAAA;UACA,KAAAlB,IAAA;QACA;UACA,KAAAiB,QAAA,CAAAE,KAAA,CAAAZ,GAAA,CAAAa,GAAA;QACA;MACA;IACA;IACAW,sBAAAC,GAAA;MACAC,OA